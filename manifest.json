{
    "manifest_version": 3,
    "name": "Schedule Extractor",
    "version": "1.1.1",
    "description": "Export school registration data to Google Calendar or Apple Calendar.", 
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "images/icon128.png"
    },
    "permissions": [
        "activeTab",   
        "scripting"     
    ]
    
}